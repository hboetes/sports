# Description: web music player
# URL:         https://github.com/navidrome/navidrome.git

name=navidrome
version=git
release=1
source=(https://github.com/navidrome/navidrome.git)

build()
{
    cd $name
    # git checkout v0.47.5
    export GOBIN=$PKG/usr/local/bin
    export PATH=$PATH:/usr/local/go/bin
    # go install
    #npm i --package-lock-only
    #npm audit fix --force
    gmake setup
    gmake buildall
}
