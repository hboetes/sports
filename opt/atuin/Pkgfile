# Description:  magical history
# Dependencies: rust protobuf-devel

name=atuin
version=git
release=1
branch=main
source=(https://github.com/atuinsh/atuin.git)

build()
{
    export PATH=$PATH:~/.cargo/bin
    cd $name
    cargo build --release
    for i in atuin; do
        install -D target/release/$i   $PKG/usr/local/bin/$i
    done
}
