# Description: p2p for music
# URL:         https://github.com/Nicotine-Plus/nicotine-plus

name=nicotine
version=git
release=2.3.2
branch=master
source=(
    https://github.com/Nicotine-Plus/nicotine-plus.git
)

build()
{
    cd $name
    python -m build --wheel --no-isolation
    python -m installer --destdir=$PKG dist/*.whl
}
