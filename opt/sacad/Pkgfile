# Description: Smart Automatic Cover Art Downloader
# URL:         https://github.com/desbma/sacad.git
# dnf install  rust cargo


name=sacad
version=git
release=1
branch=master
source=(
    https://github.com/desbma/sacad.git
)

build()
{
    cd $name
    cargo build --release
    for i in $name ${name}_r; do
        install -D target/release/$i   $PKG/usr/local/bin/$i
    done
}
